{
  "name": "anygraph",
  "displayName": "AnyGraph",
  "description": "Visualize selected data as 2D graphs in VSCode",
  "version": "0.1.0",
  "publisher": "anygraph-team",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Visualization",
    "Data Science",
    "Other"
  ],
  "keywords": [
    "graph",
    "chart",
    "visualization",
    "data",
    "plot"
  ],
  "activationEvents": [
    "onCommand:anygraph.visualizeSelection"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "anygraph",
          "title": "AnyGraph",
          "icon": "$(graph)"
        }
      ]
    },
    "views": {
      "anygraph": [
        {
          "id": "anygraph.graphView",
          "name": "AnyGraph",
          "type": "webview",
          "when": "true"
        }
      ]
    },
    "commands": [
      {
        "command": "anygraph.visualizeSelection",
        "title": "Visualize Selection",
        "category": "AnyGraph"
      },
      {
        "command": "anygraph.openGraph",
        "title": "Open Graph Viewer",
        "category": "AnyGraph"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "anygraph.visualizeSelection",
          "when": "editorHasSelection",
          "group": "anygraph"
        }
      ],
      "commandPalette": [
        {
          "command": "anygraph.visualizeSelection",
          "when": "editorHasSelection"
        },
        {
          "command": "anygraph.openGraph"
        }
      ]
    },
    "keybindings": [
      {
        "command": "anygraph.visualizeSelection",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g",
        "when": "editorTextFocus && editorHasSelection"
      }
    ]
  },
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "package": "vsce package --no-dependencies",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^2.20.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@anygraph/core": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anygraph-team/anygraph.git"
  },
  "bugs": {
    "url": "https://github.com/anygraph-team/anygraph/issues"
  },
  "homepage": "https://github.com/anygraph-team/anygraph#readme",
  "license": "MIT"
}
